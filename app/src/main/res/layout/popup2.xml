<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="102dp"
        android:layout_height="141dp"
        android:layout_margin="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/logo" />

    <EditText
        android:id="@+id/newcarpopup_marque"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Marque"
        android:inputType="none"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/newcarpopup_immatriculation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.371" />


    <EditText
        android:id="@+id/newcarpopup_immatriculation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Immatriculation"
        android:inputType="none"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/newcarpopup_prix"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.385" />


    <EditText
        android:id="@+id/newcarpopup_préstation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Préstation"
        android:inputType="none"
        android:textAlignment="center"
        app:layout_constraintBottom_toTopOf="@+id/newcarpopup_prix"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.739" />

    <EditText
        android:id="@+id/newcarpopup_prix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:ems="10"
        android:gravity="center_horizontal"
        android:hint="Prix"
        android:inputType="none"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.731" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="71dp"
        android:layout_height="58dp"
        android:layout_margin="20dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/save"
        android:textSize="12sp"
        app:backgroundTint="@color/h"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newcarpopup_prix"
        app:layout_constraintVertical_bias="0.793" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="89dp"
        android:layout_height="56dp"
        android:layout_margin="20dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/cancel"
        android:textSize="12sp"
        app:backgroundTint="@color/ha2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.894"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newcarpopup_prix"
        app:layout_constraintVertical_bias="0.786" />

</androidx.constraintlayout.widget.ConstraintLayout>